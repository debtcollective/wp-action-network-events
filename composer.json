{
    "name": "debtcollective/wp-action-network-events",
    "description": "Sync and display events from Action Network.",
    "type": "wordpress-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Debt Collective",
            "email": "pea@misfist.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WpActionNetworkEvents\\": "src/",
            "Carbon_Field_Rest_Api_Select\\": "core/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "htmlburger/carbon-fields": "^3.0",
        "misfist/carbon-field-rest-api-select": "^3.3",
        "tgmpa/tgm-plugin-activation": "^2.6",
        "htmlburger/carbon-field-number": "^3.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "wp-coding-standards/wpcs": "*",
        "automattic/phpcs-neutron-ruleset": "^3.3",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "lucatume/wp-browser": "^3.0"
    },
    "repositories": [
        {
            "type":"package",
            "package": {
              "name": "misfist/carbon-field-rest-api-select",
              "version":"3.3",
              "source": {
                  "url": "https://github.com/misfist/carbon-field-rest-api-select.git",
                  "type": "git",
                  "reference":"main"
                }
            }
        }
    ]
}
